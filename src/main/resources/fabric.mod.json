{
	"schemaVersion": 1,
	"id": "astral",
	"version": "${version}",
	"name": "Astral",
	"description": "An addon mod for Astral SMP that provides tons of custom content!",
	"authors": [ "Jaxydog", "Rat Throttleman" ],
	"contributors": [ "St4rM0N3Y", "IcePenguin", "RemasteredArch" ],
	"contact": {
		"sources": "https://github.com/Jaxydog/Astral",
		"issues": "https://github.com/Jaxydog/Astral/issues"
	},
	"license": "AGPL-3.0-or-later",
	"icon": "assets/astral/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [ "dev.jaxydog.Astral" ],
		"client": [ "dev.jaxydog.AstralClient" ],
		"server": [ "dev.jaxydog.AstralServer" ],
		"fabric-datagen": [ "dev.jaxydog.AstralDataGenerator" ]
	},
	"mixins": [ "astral.mixins.json" ],
	"accessWidener": "astral.accesswidener",
	"custom": {
		"loom:injected_interfaces": {
			"net/minecraft/class_1451": [ "dev/jaxydog/utility/SprayableEntity" ],
			"net/minecraft/class_1799": [ "dev/jaxydog/utility/AstralItemStack" ],
			"net/minecraft/class_4019": [ "dev/jaxydog/utility/SprayableEntity" ]
		}
	},
	"depends": {
		"fabricloader": ">=0.15.6",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": ">=0.91.0"
	},
	"suggests": { "cheese": ">=1.2.0" }
}
